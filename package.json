{
  "name": "divine-office-frontend",
  "private": true,
  "version": "0.0.11",
  "main": "expo-router/entry",
  "author": "Daniel Write <daniel@writefamily.com>",
  "email": "daniel@writefamily.com",
  "productName": "Divine Office",
  "scripts": {
    "web": "expo start --web",
    "start": "expo start",
    "package": "expo export -p web --output-dir web-build && node separateAssets.js",
    "simple-package": "expo export -p web --output-dir web-build",
    "lint": "expo lint",
    "electron": "electron main.js",
    "build": "electron-builder",
    "test": "jest"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^7.0.0",
    "@react-navigation/native": "^7.0.0",
    "asar": "^3.2.0",
    "axios": "^1.7.9",
    "call-bind": "^1.0.8",
    "call-bind-apply-helpers": "^1.0.1",
    "child_process": "^1.0.2",
    "detect-libc": "^2.0.3",
    "events": "^3.3.0",
    "expo": "~52.0.18",
    "expo-blur": "~14.0.1",
    "expo-constants": "~17.0.3",
    "expo-font": "~13.0.1",
    "expo-haptics": "~14.0.0",
    "expo-linear-gradient": "^14.0.1",
    "expo-linking": "~7.0.3",
    "expo-router": "4.0.11",
    "expo-splash-screen": "~0.29.18",
    "expo-status-bar": "~2.0.0",
    "expo-symbols": "~0.2.0",
    "expo-system-ui": "~4.0.6",
    "expo-web-browser": "~14.0.1",
    "express": "^4.21.2",
    "fs": "^0.0.1-security",
    "fs-extra": "^11.2.0",
    "https": "^1.0.0",
    "node": "^23.4.0",
    "npm": "^10.9.2",
    "os": "^0.1.2",
    "path": "^0.12.7",
    "portfinder": "^1.0.32",
    "process": "^0.11.10",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.5",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.1.0",
    "react-native-vector-icons": "^10.2.0",
    "react-native-web": "~0.19.13",
    "react-native-webview": "13.12.5",
    "resolve-from": "^5.0.0",
    "simple-git": "^3.27.0",
    "suncalc": "^1.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.3.12",
    "@types/react-test-renderer": "^18.3.0",
    "electron": "^33.2.1",
    "electron-builder": "^25.1.8",
    "jest": "^29.2.1",
    "jest-expo": "~52.0.2",
    "react-test-renderer": "18.3.1",
    "typescript": "^5.7.2"
  },
  "build": {
    "appId": "io.github.writedan.divineoffice",
    "files": [
      "web-build/**/*",
      "web-build/assets/**/*",
      "node_modules/**/*",
      "package.json",
      "main.js",
      "utils/**/*",
      "preload.js",
      "assets/**/*",
      "APP_ROOT.js",
      "**/*"
    ],
    "extraMetadata": {
      "main": "main.js"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "nsis"
    },
    "asarUnpack": [
      "**/node_modules/npm/**/*",
      "**/node_modules/node/**/*"
    ]
  }
}
